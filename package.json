{
  "name": "levelup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@mui/material": "^5.16.7",
    "@mui/icons-material": "^5.16.7",
    "@mui/x-date-pickers": "^7.12.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "react-router-dom": "^6.26.1",
    "date-fns": "^3.6.0",
    "recharts": "^2.8.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "CHOKIDAR_USEPOLLING=true react-scripts start",
    "start:wsl": "CHOKIDAR_USEPOLLING=true WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "build:production": "NODE_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "preview": "npx serve -s build -l 3000",
    "deploy:vercel": "vercel --prod",
    "postbuild": "echo 'Build completed successfully! Ready for deployment.'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
