{
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|manifest\\.json|favicon\\.ico|sw\\.js|offline\\.html).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}